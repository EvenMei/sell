<div class="login-register-wrapper">
    <div class="container">
        <div class="lr-box">
            <div class="row">
                <div class="col-md-6">
                    <div class="lr-img">
                        <img src="/sell/assets/img/bg/login-reg.png" alt="">
                    </div>
                </div>

